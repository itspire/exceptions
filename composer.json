{
    "name": "itspire/exceptions",
    "description": "Itspire Exceptions",
    "type": "library",
    "keywords": ["itspire", "exceptions"],
    "homepage": "http://www.itspire.fr",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan RAJKOMAR",
            "email": "ryan.rajkomar@itspire.fr"
        }
    ],
    "require": {
        "php": "~8.1",
        "doctrine/collections": "~1.6",
        "itspire/common": "~3.0",
        "jms/serializer": "~3.17",
        "nyholm/psr7": "~1.5",
        "psr/http-message": "~1.0",
        "symfony/finder": "~4.4",
        "symfony/translation": "~4.4",
        "symfony/yaml": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Itspire\\Exception\\": "src/main/php"
        }
    },
    "require-dev": {
        "ext-simplexml": "*",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "~3.x-dev",
        "symfony/phpunit-bridge": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Itspire\\Exception\\Tests\\": "src/test/php"
        }
    }
}
